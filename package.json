{
  "name": "focus-warden",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:extension": "vite build && npm run copy-extension-files",
    "copy-extension-files": "cp background.js content.js popup.js blocked.js popup.html blocked.html popup.css manifest.json rules.json icon-*.png dist/",
    "preview": "vite preview",
    "test": "vitest --exclude='**/extension.e2e.test.ts'",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --exclude='**/extension.e2e.test.ts'",
    "test:e2e": "vitest run --testTimeout=60000 src/test/extension.e2e.test.ts",
    "test:coverage": "vitest run --coverage --exclude='**/extension.e2e.test.ts'",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "puppeteer": "^24.33.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.1",
    "vitest": "^3.2.4"
  }
}
